<script type="text/javascript">
  $('.back-fixed').on('mouseenter', function() {
    $(this).addClass('animated pulse');
  }).on('mouseleave', function() {
    $(this).removeClass('animated pulse');
  })

  $('.icons .box').on('mouseenter', function() {
    $(this).find('.img-wrap').addClass('animated pulse');
  }).on('mouseleave', function() {
    $(this).find('.img-wrap').removeClass('animated pulse');
  })
</script>
