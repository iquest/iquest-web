<div>
		<script>
		window.onmessage = function(e) {
			if (e.data.type === 'myHeight') {
					document.getElementById('questionareIframe').setAttribute('height', e.data.height)
			}
			if (e.data.type === 'scrollToTop') {
				window.scrollTo(0, 250);
			}
		};</script>

		<iframe src="https://iquest-questionnaire.netlify.app"
		id='questionareIframe'
		frameborder="0"
		scrolling="no" height="100%" width="100%"
		></iframe>
 			<div className="ready-second-block">
        <p className="header2 text-center">
          <span className="color-dark-green">Spokojený kolega -</span> náš cíl
        </p>
        <div className="flex justify-center">
          <div className=" ready-pick-box ready-pick-box-front">
            <div className="ready-pick-box-background">
              <div className="ready-pick-box-feature"></div>
              <div className="header3">React vývojář</div>
              <strong className="color-dark-green">Senior &bull; Junior</strong>
              <br />
              <strong className="color-grey flex align-center m-b-1">
                <img className="m-r-1" alt="marker" src="/images/place.svg" />{" "}
                Praha / Brno / Externě
              </strong>
              <div className="ready_link-margin">
                <a
                  className="button-primary link"
                  href="https://www.iquest.cz/react-vyvojar"
                >
                  Zajímá mě to
                </a>
              </div>
              <br />
            </div>
          </div>
          <div className=" ready-pick-box ready-pick-box-back">
